  fhir:action ( [
     fhir:title [ fhir:v "PrEP visit business process" ] ;
     fhir:description [ fhir:v "To provide the client with PrEP as a prevention choice for people at elevated risk for HIV acquisition, as part of a combination of HIV prevention approaches, and during a clinic visit. Fig. 10 shows the flow of the PrEP visit proces" ] ;
     fhir:participant ( [
       fhir:type [ fhir:v "patient" ] ;
       fhir:role [
         fhir:coding ( [
           fhir:code [ fhir:v "client" ]
         ] )
       ]
     ] [
       fhir:type [ fhir:v "practitioner" ] ;
       fhir:role [
         fhir:coding ( [
           fhir:code [ fhir:v "TrainedLayProvider" ]
         ] )
       ]
     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "HIV.C.1" ] ;
       fhir:title [ fhir:v "Determine reason for visit" ] ;
       fhir:description [ fhir:v "Search for client details in the system and determine reason for visit" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.2" ] ;
       fhir:title [ fhir:v "Take vital signs" ] ;
       fhir:description [ fhir:v "Vital signs, such as blood pressure and weight, may be taken and recorded." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.3" ] ;
       fhir:title [ fhir:v "Capture or update client history" ] ;
       fhir:description [ fhir:v "Discuss history with client and review records. Examples of history to take include other diagnoses, medications (including any use of ART), and partner’s HIV status " ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.4" ] ;
       fhir:title [ fhir:v "Test for HIV using testing algorithm " ] ;
       fhir:description [ fhir:v "Existing HIV infection should be ruled out by testing and should be performed the same day that PrEP is started, as per a national HIV testing algorithm." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.6" ] ;
       fhir:title [ fhir:v "Post-test package of services" ] ;
       fhir:description [ fhir:v "If the client tested positive for HIV, the client should be counselled and linked to care, based on an essential post-test service package." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.7" ] ;
       fhir:title [ fhir:v "Check suitability for PrEP or PEP" ] ;
       fhir:description [ fhir:v "For new visits, check the client’s suitability for PrEP or post-exposure prophylaxis (PEP). " ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.8" ] ;
       fhir:title [ fhir:v "Suitable for PrEP or PEP?" ] ;
       fhir:description [ fhir:v "• If the initial HIV serology test result is non-reactive (negative), the client meets suitability criteria, and there are no history, signs or symptoms of an acute viral syndrome, the person could be offered and, if desired, initiated on PrEP." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.9" ] ;
       fhir:title [ fhir:v "Discuss PrEP or PEP" ] ;
       fhir:description [ fhir:v "PrEP should be used during periods at elevated risk for HIV acquisition and can be stopped during periods of low or no risk, depending on PrEP formulation and product" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.10" ] ;
       fhir:title [ fhir:v "Counsel on risk and prevention" ] ;
       fhir:description [ fhir:v "• If PEP and PrEP are not suitable for the client, such as the client declining medication, the provider may counsel on risk and other prevention options, as well as promote and provide condoms." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.11" ] ;
       fhir:title [ fhir:v "Family planning counselling" ] ;
       fhir:description [ fhir:v "If appropriate, provide or link to support for family planning counselling." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.12" ] ;
       fhir:title [ fhir:v "STI services" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.17" ] ;
       fhir:title [ fhir:v "Determine recommended tests" ] ;
       fhir:description [ fhir:v "In addition to HIV testing, a package of screenings is recommended for new and continuing PrEP users, specific to the chosen prevention option as per national guidelines" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "patient" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "client" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.19" ] ;
       fhir:title [ fhir:v "Conduct other screenings and tests" ] ;
       fhir:description [ fhir:v "The provider may perform other screenings, including rapid diagnostics for STIs, and physical examinations. Diagnoses for these may reference other digital adaptation kits." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.20" ] ;
       fhir:title [ fhir:v "Perform additional diagnostics?" ] ;
       fhir:description [ fhir:v "Are additional diagnostics (beyond an HIV test) to be performed?" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.21" ] ;
       fhir:title [ fhir:v "Diagnostics" ] ;
       fhir:description [ fhir:v "If diagnostics are to be performed, this may take place at this point. Or this may take place at another time during the process, or after, if the client needs to go to a lab." ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.22" ] ;
       fhir:title [ fhir:v "Counsel on effective use" ] ;
       fhir:description [ fhir:v "PrEP provides high levels of protection in people who take PrEP regularly. Provide counselling on how to use PrEP effectively" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.23" ] ;
       fhir:title [ fhir:v "Prescribe or administer PrEP or PEP" ] ;
       fhir:description [ fhir:v "Prescribe or administer PrEP, depending upon the PrEP formulation discussed" ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] [
       fhir:id [ fhir:v "HIV.C.24" ] ;
       fhir:title [ fhir:v "Schedule follow-up" ] ;
       fhir:description [ fhir:v "A person using PrEP should have regular HIV testing, such as every three months. " ] ;
       fhir:participant ( [
         fhir:type [ fhir:v "practitioner" ] ;
         fhir:role [
           fhir:coding ( [
             fhir:code [ fhir:v "TrainedLayProvider" ]
           ] )
         ]
       ] )
     ] )
  ] ) . # 

